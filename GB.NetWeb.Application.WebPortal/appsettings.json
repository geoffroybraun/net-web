{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "TaskHandlerConfiguration": {
    "MaxRetriesCount": 5,
    "MaxExceptionsCount": 3,
    "TimeBetweenBreaksInMilliseconds": 25000
  },
  "HttpClientConfiguration": {
    "BaseAddress": "https://localhost:8001",
    "AcceptHeaderValue": "application/json"
  }
}
